    {
        "id": "d29b06beb66fe924",
        "type": "function",
        "z": "48b832ad4dead599",
        "name": "RegistrarPasse",
        "func": "// RegistrarPasse - saÃ­da mÃºltipla\nlet now = Date.now();\nlet last = flow.get('lastPassTime') || null;\nlet passCount = flow.get('passCount') || 0;\npassCount += 1;\nflow.set('passCount', passCount);\nflow.set('lastPassTime', now);\n\nlet intervals = flow.get('intervals') || [];\nlet intervalSec = null;\nif (last !== null) {\n    intervalSec = (now - last) / 1000.0;\n    intervals.push(intervalSec);\n    if (intervals.length > 100) { intervals.shift(); }\n    flow.set('intervals', intervals);\n}\n\nlet avgInterval = 0;\nif (intervals.length > 0) {\n    let sum = intervals.reduce((a,b) => a+b, 0);\n    avgInterval = sum / intervals.length;\n}\n\nlet counts = flow.get('playerCounts') || {};\nlet selectedPlayer = flow.get('selectedPlayer') || 'Desconhecido';\ncounts[selectedPlayer] = (counts[selectedPlayer] || 0) + 1;\nflow.set('playerCounts', counts);\n\n// determinar quem mais passa\nlet topPlayer = selectedPlayer;\nlet topCount = counts[selectedPlayer];\nfor (let p in counts) {\n    if (counts[p] > topCount) { topPlayer = p; topCount = counts[p]; }\n}\nlet status = 'Aguardando 2Âº passe';\nif (intervalSec !== null) {\n    if (intervalSec <= 5) {\n        status = 'ðŸŸ¢ Intenso';\n    } else if (intervalSec <= 15) {\n        status = 'ðŸŸ¡ Moderado';\n    } else {\n        status = 'ðŸ”´ Jogo Lento';\n    }\n}\n\n// atualizar dados do grÃ¡fico (intervalos)\nlet chartLabels = flow.get('chartLabels') || [];\nlet chartData = flow.get('chartData') || [];\nlet label = new Date(now).toLocaleTimeString();\nchartLabels.push(label);\nchartData.push(intervalSec === null ? 0 : Math.round(intervalSec * 100)/100);\nif (chartLabels.length > 20) { chartLabels.shift(); chartData.shift(); }\nflow.set('chartLabels', chartLabels);\nflow.set('chartData', chartData);\n\n// preparar mensagens (5 saÃ­das)\nlet msgTotal = { payload: passCount };\nlet msgAvg = { payload: (avgInterval === 0 ? 'â€”' : Number(avgInterval.toFixed(2))) };\nlet msgTop = { payload: (topPlayer + ' (' + topCount + ')') };\nlet msgStatus = { payload: status };\nlet msgChart = { payload: [ { series: ['Intervalo (s)'], data: [ chartData.map(x=>x) ], labels: chartLabels } ] };\n\nreturn [msgTotal, msgAvg, msgTop, msgStatus, msgChart];\n",
